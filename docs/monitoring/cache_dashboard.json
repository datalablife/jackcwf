{
  "dashboard": {
    "title": "LangChain Semantic Cache Monitoring",
    "description": "Real-time monitoring dashboard for semantic cache performance and metrics",
    "tags": ["cache", "performance", "ai-optimization"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Cache Hit Rate (%)",
        "type": "gauge",
        "targets": [
          {
            "expr": "llm_cache_hit_rate{model=\"claude-3-5-sonnet-20241022\"}",
            "legendFormat": "Hit Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "orange", "value": 20},
                {"color": "yellow", "value": 40},
                {"color": "green", "value": 60}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Cache Size (Entries)",
        "type": "stat",
        "targets": [
          {
            "expr": "llm_cache_size_entries{model=\"claude-3-5-sonnet-20241022\"}",
            "legendFormat": "Entries"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Cache Table Size (Bytes)",
        "type": "stat",
        "targets": [
          {
            "expr": "llm_cache_table_size_bytes{model=\"claude-3-5-sonnet-20241022\"}",
            "legendFormat": "Size"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Cache Hits vs Misses (Total)",
        "type": "piechart",
        "targets": [
          {
            "expr": "llm_cache_hits_total{model=\"claude-3-5-sonnet-20241022\", cache_type=\"semantic\"}",
            "legendFormat": "Hits"
          },
          {
            "expr": "llm_cache_misses_total{model=\"claude-3-5-sonnet-20241022\", cache_type=\"semantic\"}",
            "legendFormat": "Misses"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Query Latency Comparison (ms)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(llm_cache_hit_latency_ms_bucket{model=\"claude-3-5-sonnet-20241022\"}[5m]))",
            "legendFormat": "Cache Hit (p95)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_cache_miss_latency_ms_bucket{model=\"claude-3-5-sonnet-20241022\"}[5m]))",
            "legendFormat": "Cache Miss (p95)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Hit Latency Distribution (ms)",
        "type": "histogram",
        "targets": [
          {
            "expr": "rate(llm_cache_hit_latency_ms_bucket{model=\"claude-3-5-sonnet-20241022\"}[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Miss Latency Distribution (ms)",
        "type": "histogram",
        "targets": [
          {
            "expr": "rate(llm_cache_miss_latency_ms_bucket{model=\"claude-3-5-sonnet-20241022\"}[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Query Component Latencies (ms)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(llm_embedding_latency_ms_bucket{model=\"claude-3-5-sonnet-20241022\"}[5m]))",
            "legendFormat": "Embedding (p95)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_vector_search_latency_ms_bucket{model=\"claude-3-5-sonnet-20241022\"}[5m]))",
            "legendFormat": "Vector Search (p95)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_generation_latency_ms_bucket{model=\"claude-3-5-sonnet-20241022\"}[5m]))",
            "legendFormat": "Generation (p95)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "Cache Distance (Semantic Similarity)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(llm_cache_distance_bucket{model=\"claude-3-5-sonnet-20241022\"}[5m]))",
            "legendFormat": "Distance (p95)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 10,
        "title": "Cached Responses Served (Counter)",
        "type": "stat",
        "targets": [
          {
            "expr": "llm_cached_responses_served{model=\"claude-3-5-sonnet-20241022\"}",
            "legendFormat": "Responses"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      }
    ],
    "refresh": "30s",
    "schemaVersion": 35,
    "style": "dark",
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "datasourceRegex": "Prometheus",
          "current": {
            "value": "Prometheus",
            "text": "Prometheus"
          }
        }
      ]
    }
  }
}
